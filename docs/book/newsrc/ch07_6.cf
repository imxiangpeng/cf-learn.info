bundle agent test
{
  vars:
      "tpat1" string => "line1.*";    # <1>
      "tstr1" string => "new line 1";
      "tpat2" string => "line2.*";
      "tstr2" string => "new line 2";

  files:
      "$(G.testfile).actual"   # <2>
        create => "false",
        edit_line => replace_or_add("$(test.tpat1)", "$(test.tstr1)");
      "$(G.testfile).actual"
        create => "false",
        edit_line => replace_or_add("$(test.tpat2)", "$(test.tstr2)");
}
