bundle common g
{
 vars:
  testrun::
   "class_file" string => "/tmp/classes.txt";
  !testrun::
   "class_file" string => "/var/cfengine/site/classes.txt";
  any::
   "class_strs"
     slist => readstringlist("$(class_file)",
                             "#.*?\n", "\s+", "inf", "inf");
 classes:
   "$(class_strs)"  expression => "any";
}
